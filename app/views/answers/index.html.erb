<h1>Answers</h1>

<table>
  <thead>
    <tr>
      <th>App form</th>
      <th>Question</th>
      <th>Answer</th>
    </tr>
  </thead>

  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td><%= answer.app_form %></td>
        <td><%= answer.question %></td>
        <td><%= answer.answer %></td>
      </tr>
    <% end %>
  </tbody>
</table>
